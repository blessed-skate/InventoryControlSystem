create table TACISASSET (
	FIIDASSET INT,
	FIIDLEDGER INT,
	FIIDASSETTYPE TINYINT,
	FCDESCRIPTION VARCHAR(100) not null,
	FCBRAND VARCHAR(50) not null default 'Sin marca',
	FCMODEL VARCHAR(50) not null default 'Sin modelo',
	FCSERIALNUMBER VARCHAR(50) not null default 'Sin numero de serie',
	FIIDMATERIAL TINYINT,
	FCCOLOR VARCHAR(100),
	FCSUPPLIER VARCHAR(50) not null default 'Proveedor no identificado',
	FCGENERALMANAGER VARCHAR(100),
	FCDIRECTLYRESPONSIBLE VARCHAR(100),
	FITAG BIGINT,
	FCBILL VARCHAR(30) not null default 'Sin factura',
	FDBILLINGDATE date not null default '01/06/2015',
	FCLOCATION VARCHAR(50),
	FDBIRTH date,
	
	FCPASSWORD VARCHAR(100), 
	FCNAME VARCHAR(50), 
	FCLASTNAME VARCHAR(50), 
	FLSEX CHAR(1), 
	FDBIRTH date
);