<script type="text/javascript"
	src="js/dhtmlx/sources/dhtmlxForm/codebase/dhtmlxform.js"></script>
<link rel="stylesheet" type="text/css"
	href="js/dhtmlx/sources/dhtmlxForm/codebase/skins/dhtmlxform_dhx_web.css" />
<script>
	var myForm;
	function doOnLoad() {
		var window_1 = .createWindow("loginForm", 0, 0, 300, 400);
		form_1.loadStruct('./data/form.xml');

		window_1.setText('Ingreso');
		window_1.centerOnScreen();
		window_1.button('park').hide();
		window_1.button('minmax').hide();
		window_1.button('close').hide();

		
		myForm = window_1.attachForm();
		myForm.loadStruct("xml/login_form.xml");
		myForm.attachEvent("onButtonClick", function(id){
		    //alert("Button with name "+id+" was clicked");
		    if(id == "login"){
		    	login_form.submit();
		    }
		});
	}
</script>
<style>
div#homeTabbar {
