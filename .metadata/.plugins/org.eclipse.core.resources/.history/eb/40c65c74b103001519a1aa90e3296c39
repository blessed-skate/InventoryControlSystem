function showResponseXmlAlert(msg){
	insert_form.send("myAsset.do?method=insertAsset","post",function(loader, response){
	  alert("Saved|"+loader+"|"+response);
	});
}

function showError(msg){
	insert_form.send("myAsset.do?method=insertAsset","post",function(loader, response){
	  alert("Saved|"+loader+"|"+response);
	});
}

