CREATE TABLE CISDB.CTCISASSETTYPE 
( 
  FIIDASSETTYPE INT(3) NOT NULL AUTO_INCREMENT
, FCDESCRIPTION VARCHAR(100) NOT NULL 
, FDREGISTERDATE TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP
, FDLASTUPDATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP 
, CONSTRAINT PRIMARY KEY ( FIIDASSETTYPE ) );

CREATE INDEX CISDB.CTCISASSETTYPE_INDEX1 ON CISDB.CTCISASSETTYPE ( FIIDASSETTYPE);