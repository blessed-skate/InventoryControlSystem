DROP TABLE CISDB.TACISAUTH;

CREATE TABLE CISDB.TACISAUTH 
( 
  IDUSER SMALLINT(5) NOT NULL  
, USERNAME VARCHAR(50) NOT NULL  
, PASSWORD VARCHAR(100) NOT NULL 
, AUTHORITY VARCHAR(50) NOT NULL 
, ENABLED ENUM(1,0) NOT NULL  
, FDREGISTERDATE TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP
, FDLASTUPDATE TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP 
, CONSTRAINT PRIMARY KEY ( FIIDUSER ) );

CREATE INDEX TACISAUTH_INDEX1 ON CISDB.TACISAUTH ( FIIDUSER ASC);
