dhtmlxError.catchError("LoadXML", function(a, b, data) {
	alert('${loading_err}' + '\n' + '${reload_msg}');
});

function showResponseXmlAlert(msg){
	dhtmlx.alert({title: "Error", type: "alert-error", text: msg});
	dhtmlx.alert({title: "Warning", type: "alert-warning", text: msg});
	dhtmlx.alert({title: "Alert", type: "alertÂ¡", text: msg});
	dhtmlx.alert({title: "Confirm", type: "confirm", text: msg, 
	callback: function(result){
		dhtmlx.message("Result: " + result)
	}	
	});
}

function showError(msg){
	dhtmlx.alert({title: "Error", text: msg});
}

