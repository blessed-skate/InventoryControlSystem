package mx.com.icsc.common.util;

import java.io.StringReader;

import javax.xml.bind.JAXBContext;
import javax.xml.bind.Unmarshaller;

import mx.com.icsc.common.AssetResponse;

public class XmlFactory {
	
	public static String getXml(AssetResponse object){
		JAXBContext jaxbContext = JAXBContext.newInstance(AssetResponse.class);
		Unmarshaller unmarshaller = jaxbContext.createUnmarshaller();

		StringReader reader = new StringReader("xml string here");
		AssetResponse response = (AssetResponse) unmarshaller.unmarshal(AssetResponse);
		
		return null;
	}

}
