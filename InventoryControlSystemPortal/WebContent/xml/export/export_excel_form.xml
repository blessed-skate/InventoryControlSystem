<?xml version="1.0" encoding="UTF-8"?>
<items>
	<item type="settings" position="label-left" labelWidth="130" inputWidth="200" />
	<item type="input" label="Nombre del archivo" name="fileName"
		maxLength="50" required="true" />
	<item type="select" label="Extension" name="extension">
		<option text="*.xls" value="xls" />
		<option text="*.xlsx" value="xlsx" />
	</item>
	<item type="checkbox" label="Cabecera" name="header" checked="true">
		<note width="170">Desea exportar el titulo de las columnas</note>
	</item>
	<!-- <item type="template" label="Excel" name="download" format="doLink" 
		value="Descargar"/> -->

	<item type="button" name="getAssetGridExcel" value="Exportar"
		width="100" />
</items>